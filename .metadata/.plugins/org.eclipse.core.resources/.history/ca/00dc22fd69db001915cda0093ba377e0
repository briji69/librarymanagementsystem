public interface AdminServ {

	boolean addLogin(Login user);
	Login login(int id, String password);
	boolean deleteOwner(int id);
	Login updateCheckOwner(int id);
	boolean updateLogin(Login ownerLogin);
	List<Login> displayOwners();
	List<Login> displayCustomers();
