package com.capgemini.busschedulingcollections.beans;

public class Bus {
	private int busNo;
	private String busName;
	private String source;
	private String destination;
	private int seatsAvailabile;
	
	//setters and getters
	public int getBusNo() {
		return busNo;
	}
	public void setBusNo(int busNo) {
		this.busNo = busNo;
	}
	public String getBusName() {
		return busName;
	}
	public void setBusName(String busName) {
		this.busName = busName;
	}
	public String getSource() {
		return source;
	}
	public void setSource(String source) {
		this.source = source;
	}
	public String getDestination() {
		return destination;
	}
	public void setDestination(String destination) {
		this.destination = destination;
	}
	public int getSeatsAvailabile() {
		return seatsAvailabile;
	}
	public void setSeatsAvailabile(int seatsAvailabile) {
		this.seatsAvailabile = seatsAvailabile;
	}
	
}//end class
